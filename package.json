{
	"name": "react-mermaid1",
	"private": true,
	"version": "0.0.4",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"format": "biome format --write . && dprint fmt",
		"check": "biome check --write . && dprint check",
		"actionlint": "docker run --rm -v \"$(pwd):$(pwd)\" -w \"$(pwd)\" rhysd/actionlint",
		"parser:name": "peggy -o lib/nameParser.js -m --dts src/name.peggy",
		"parser:names": "peggy -o lib/namesParser.js -m --dts src/names.peggy",
		"parser:calc": "peggy -o lib/calcParser.js -m --dts src/calc.peggy"
	},
	"dependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.27.0"
	},
	"devDependencies": {
		"@types/node": "^22.8.4",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react-swc": "^3.7.1",
		"typescript": "^5.6.3",
		"vite": "^5.4.10",
		"vite-plugin-cdn-import": "^1.0.1",
		"peggy": "^4.1.1",
		"ts-pegjs": "^4.2.1"
	}
}
